#!/bin/bash
set -e

IMAGEID=$( docker images -q openebs/tests-sysbench-mysql )

if [ ! -z "${DNAME}" ] && [ ! -z "${DPASS}" ]; 
then 
  docker login -u "${DNAME}" -p "${DPASS}"; 
  #Push to docker hub repository with latest tag
  docker tag ${IMAGEID} openebs/tests-sysbench-mysql:latest
  docker push openebs/tests-sysysbench-mysqll:latest; 
else
  echo "No docker credentials provided. Skip uploading openebs/tests-tpcc-client:latest to docker hub"; 
fi;
